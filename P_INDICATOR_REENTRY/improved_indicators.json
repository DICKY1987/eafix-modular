  "indicators": {
    "RATE_DIFF_MOMO_Z": {
      "id": "RATE_DIFF_MOMO_Z",
      "name": "2-Year Rate Differential Momentum",
      "category": "rate_differentials",
      "subcategory": "momentum",
      "description": {
        "concept": "2-Year Rate Differential Momentum (US2Y−DE2Y)",
        "computation": "Compute z-score of the change in short-rate differential Δ(US2Y−DE2Y) over lookback L (e.g., 5–20 bars).",
        "signal_logic": "If z ≥ +z_th → USD-positive bias (e.g., short EURUSD); if z ≤ −z_th → USD-negative bias.",
        "notes": "For other pairs, swap relevant sovereign 2Y yields."
      },
      "output": {
        "type": "z_score",
        "range": "unbounded",
        "interpretation": "Higher values indicate stronger USD momentum"
      },
      "thresholds": {
        "type": "zscore",
        "upper_threshold": 1.0,
        "lower_threshold": -1.0,
        "hysteresis": 0.2,
        "persistence_bars": 1
      },
      "data_inputs": {
        "external_series": ["US2Y", "DE2Y"],
        "provider": "FRED/ECB",
        "resample_frequency": "H1",
        "transform": "diff_then_z",
        "symbol_scope": ["EURUSD"],
        "timeframe": "H1"
      },
      "usage": {
        "signal_strength": "medium",
        "frequency": "intraday",
        "best_sessions": ["london", "ny_overlap"]
      }
    },
    
    "VIX_Z": {
      "id": "VIX_Z", 
      "name": "Volatility Spillover (VIX)",
      "category": "volatility_spillover",
      "subcategory": "equity_derived",
      "description": {
        "concept": "Volatility Spillover (VIX)",
        "computation": "z-score of ΔVIX with optional 1–3 bar lags.",
        "signal_logic": "High VIX_Z gates risk-off plays (long JPY/CHF; short AUD/NZD).",
        "notes": "Use as filter."
      },
      "output": {
        "type": "z_score",
        "range": "unbounded", 
        "interpretation": "Higher values indicate risk-off sentiment"
      },
      "thresholds": {
        "type": "zscore",
        "upper_threshold": 1.0,
        "lower_threshold": null,
        "hysteresis": 0.2,
        "persistence_bars": 1
      },
      "data_inputs": {
        "external_series": ["^VIX"],
        "provider": "CBOE",
        "resample_frequency": "M15",
        "transform": "diff_then_z",
        "symbol_scope": ["USDJPY", "AUDUSD", "NZDUSD", "USDCHF"],
        "timeframe": "M15"
      },
      "usage": {
        "signal_strength": "high",
        "frequency": "intraday",
        "best_sessions": ["all"],
        "filter_type": "risk_gate"
      }
    },

    "FRI_MOVE_PCT": {
      "id": "FRI_MOVE_PCT",
      "name": "Friday Time-Window Move",
      "category": "time_based_patterns", 
      "subcategory": "weekly_patterns",
      "description": {
        "concept": "Friday time-window absolute percent move (07:30→14:00 CT)",
        "computation": "% change between price at 07:30 and 14:00 (America/Chicago) on Fridays.",
        "signal_logic": "After 14:00, if abs(%Δ) ≥ threshold, emit one-shot LONG if Δ>0, SHORT if Δ<0.",
        "notes": "Snapshot candles at window boundaries."
      },
      "output": {
        "type": "percent",
        "range": "unbounded",
        "interpretation": "Absolute percentage move during Friday window"
      },
      "thresholds": {
        "type": "percent_change",
        "abs_change_gte": 1.0,
        "direction": "both",
        "one_shot": true,
        "window": {
          "timezone": "America/Chicago",
          "start_time": "07:30",
          "end_time": "14:00", 
          "days": ["FRI"]
        }
      },
      "data_inputs": {
        "price_source": "broker_bid",
        "symbol_scope": ["EURUSD", "GBPUSD", "USDJPY"],
        "timeframe": "M5",
        "timezone_offsets": {
          "broker_gmt_offset": 2,
          "local_gmt_offset": -6
        }
      },
      "usage": {
        "signal_strength": "high",
        "frequency": "weekly",
        "best_sessions": ["friday_close"],
        "execution_style": "momentum_breakout"
      }
    },

    "MOI_0_1": {
      "id": "MOI_0_1",
      "name": "Market Obviousness Index",
      "category": "risk_regime",
      "subcategory": "market_psychology",
      "description": {
        "concept": "Market Obviousness Index",
        "computation": "Blend breakout clarity, ATR vs range, MA alignment, news impact, volume confirm into 0–1 score.",
        "signal_logic": "MOI>0.8 → fade obvious move; 0.3–0.8 neutral; <0.3 confusion (reduce size).",
        "notes": "Weights depend on session."
      },
      "output": {
        "type": "probability_0_1",
        "range": "0.0 to 1.0",
        "interpretation": "Higher values indicate more obvious market setups"
      },
      "thresholds": {
        "type": "band",
        "upper_threshold": 0.8,
        "lower_threshold": 0.3,
        "hysteresis": 0.05
      },
      "data_inputs": {
        "components": ["ATR_ratio", "MA_stack", "news_score", "vol_confirm"],
        "resample_frequency": "M15",
        "timeframe": "M15",
        "blend_method": "weighted"
      },
      "usage": {
        "signal_strength": "medium",
        "frequency": "intraday",
        "best_sessions": ["all"],
        "filter_type": "contrarian_signal"
      }
    },

    "EXP_PIN_SCORE": {
      "id": "EXP_PIN_SCORE",
      "name": "Options Expiry Pin Score",
      "category": "expiry_effects",
      "subcategory": "pin_effects",
      "description": {
        "concept": "Options expiry pin reversion score",
        "computation": "Pin strength of spot vs large strikes into expiry (0–1).",
        "signal_logic": "High pin score → mean-reversion toward strike.",
        "notes": "Requires options OI by strike."
      },
      "output": {
        "type": "probability_0_1",
        "range": "0.0 to 1.0",
        "interpretation": "Higher values indicate stronger pin effects"
      },
      "thresholds": {
        "type": "band",
        "upper_threshold": 0.7,
        "lower_threshold": 0.3
      },
      "data_inputs": {
        "provider": "expiry_service",
        "update_frequency": "intraday",
        "required_data": ["options_oi", "strike_levels"]
      },
      "usage": {
        "signal_strength": "high",
        "frequency": "event_driven",
        "best_sessions": ["expiry_day"],
        "execution_style": "mean_reversion"
      }
    }
  },
  

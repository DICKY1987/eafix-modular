{
  "_doc_id": "DOC-CONFIG-0128",

  "doc_id": "DOC-CONTRACT-0011",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecutionReport",
  "version": "1.0",
  "description": "Order execution result from broker",
  "type": "object",
  "properties": {
    "order_id": {
      "type": "string",
      "format": "uuid",
      "description": "Internal order identifier"
    },
    "broker_order_id": {
      "type": "string",
      "description": "Broker-assigned order identifier"
    },
    "status": {
      "type": "string",
      "enum": [
        "PENDING",
        "PARTIAL",
        "FILLED",
        "CANCELLED",
        "REJECTED"
      ],
      "description": "Order execution status"
    },
    "filled_qty": {
      "type": "number",
      "minimum": 0,
      "description": "Quantity filled"
    },
    "remaining_qty": {
      "type": "number",
      "minimum": 0,
      "description": "Quantity remaining"
    },
    "avg_price": {
      "type": "number",
      "minimum": 0,
      "description": "Average execution price"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "Execution timestamp"
    },
    "commission": {
      "type": "number",
      "description": "Commission charged"
    },
    "error_message": {
      "type": "string",
      "description": "Error message if execution failed"
    }
  },
  "required": [
    "order_id",
    "status",
    "timestamp"
  ],
  "additionalProperties": false
}
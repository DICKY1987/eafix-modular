{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "IndicatorVector",
  "version": "1.1",
  "description": "Technical indicator calculation results",
  "type": "object",
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "UTC timestamp of calculation"
    },
    "symbol": {
      "type": "string",
      "pattern": "^[A-Z]{6}$",
      "description": "Currency pair symbol"
    },
    "features": {
      "type": "object",
      "description": "Dictionary of indicator name to value",
      "patternProperties": {
        "^[a-zA-Z0-9_]+$": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "window_version": {
      "type": "string",
      "description": "Version identifier for calculation window"
    },
    "metadata": {
      "type": "object",
      "properties": {
        "calculation_time_ms": {
          "type": "number",
          "description": "Time taken for calculation"
        },
        "data_points": {
          "type": "integer",
          "description": "Number of data points used"
        }
      },
      "additionalProperties": false
    }
  },
  "required": ["timestamp", "symbol", "features", "window_version"],
  "additionalProperties": false
}